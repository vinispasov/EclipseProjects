package MockExam;
import java.util.Scanner;
public class Task03 {
	public static void main(String[] args) {
		
	Scanner sc= new Scanner(System.in);
	
	int n=sc.nextInt();
	int a=(n/10)/10%10;
	System.out.println(a);
	int b=n/10%10;
	System.out.println(b);
	int c=n%10;
	System.out.println(c);
	
	}
	
}
