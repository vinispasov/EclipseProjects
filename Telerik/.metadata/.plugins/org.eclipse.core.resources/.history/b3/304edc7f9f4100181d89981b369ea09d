package MockExamVariant2;
import java.util.Scanner;
public class Game {
	
	public static void main(String[] args) {
		
		Scanner sc=new Scanner(System.in);
		
		int num=sc.nextInt();
		
		int a=(num/10)/10%10;
		int b=num/10%10;
		int c=num%10;
		
		int product=a*b*c;
		int sum=a+b+c;
		if (product>sum) {
			System.out.println(product);
		}
		else {
			System.out.println(sum);
		}
	}

}
