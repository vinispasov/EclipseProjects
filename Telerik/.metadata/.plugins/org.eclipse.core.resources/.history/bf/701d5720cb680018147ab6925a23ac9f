import java.util.Scanner;
public class Factoriel {
	
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		
		long n=sc.nextLong();
		
		System.out.println(factoriel(n));
	}
	
	static long factoriel(long n) {
		 if (n==1) {
				return 1;
			}
		return n*factoriel(n-1);
	}

}
