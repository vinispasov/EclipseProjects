import java.util.Scanner;
public class Factoriel {
	
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		
		int n=sc.nextInt();
		
		System.out.println(factoriel(n));
	}
	
	static int factoriel(int n) {
		
		int result=n*factoriel(n-1);
		 if (n==1) {
				return 1;
			}
		return result;
	}

}
