
public class Page {
	
	private String title;
	private String text;
	
	
	//methods
	
	void addText(String text){
		this.text=text;
	}
	
	void removeText() {
		this.text=this.text.replace((this.text),null);
	}
	void pageView() {
		System.out.println(this.title);
		System.out.println(this.text);
		
	}

}
