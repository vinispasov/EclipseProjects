import java.util.Scanner;

public class task8 {
public static void main(String[] args) {
	Scanner sc=new Scanner(System.in);
	
	int number=sc.nextInt();
	
		int a=number%10;
		int b=(number%100)%10;
		int c=number%1000;
		int d=number%10000;
		System.out.println(a);
		System.out.println(b);
		System.out.println(c);
		System.out.println(d);
	
	
}
}
