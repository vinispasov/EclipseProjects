
public class task12 {
	

	public static void main(String[] args) {
		
		
		
		String number ="100"; 
		int num=Integer.parseInt(number);;
		int a=number.charAt(0)-'0';   
		int b=number.charAt(1)-'0';    
		int c=number.charAt(2)-'0';
		
		for (int i = 0; i <=8; i++) {
			if (!(a==b&&b==c)) {
				
				System.out.println(a+""+b+""+c);
			}
			for (int j =0; j <=9; j++) {
				if (!(a==b&&b==c)) {
					
					System.out.println(a+""+b+""+c);
				}
				for (int j2 =0; j2 <=9; j2++) {
					if (!(a==b&&b==c)) {
						
						System.out.println(a+""+b+""+c);
					}
					
					
					c++;
				}
				c=0;
				b++;
			}
			b=0;
			a++;
		}
		
	}
	}


