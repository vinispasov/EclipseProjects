
public class task12 {
	

	public static void main(String[] args) {
		
		
		
		String number ="999"; 
		int num=Integer.parseInt(number);;
		int a=number.charAt(0)-'0';   
		int b=number.charAt(1)-'0';    
		int c=number.charAt(2)-'0';
		
		for (int i =1; i <=num; i++) {
			if ((a==b&&b==c)) {
				System.out.println(i);
			}
		}
		
		
	}
	}


