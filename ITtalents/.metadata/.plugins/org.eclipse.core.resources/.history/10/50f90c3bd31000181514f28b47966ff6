import Vignette.Vignette;

public abstract class Vehicle {
	private String model;
	private int yearOfProducing;
	private boolean isGluedVignette;
	private Vignette vignette;
	
	public Vehicle(String model,int yearOfProducing) {
		this.model=model;
		this.yearOfProducing=yearOfProducing;
		this.isGluedVignette=false;
	}
	
	//getters and setters

	public String getModel() {
		return model;
	}

	public void setModel(String model) {
		this.model = model;
	}

	public int getYearOfProducing() {
		return yearOfProducing;
	}

	public void setYearOfProducing(int yearOfProducing) {
		this.yearOfProducing = yearOfProducing;
	}

	public boolean isGluedVignette() {
		return isGluedVignette;
	}

	public void setGluedVignette(boolean isGluedVignette) {
		this.isGluedVignette = isGluedVignette;
	}

	public Vignette getVignette() {
		return vignette;
	}

	public void setVignette(Vignette vignette) {
		this.vignette = vignette;
	}
	
	

}
