
public class task12 {
	

	public static void main(String[] args) {
		
		
		
		String number ="100"; 
		int num=Integer.parseInt(number);;
		int a=number.charAt(0)-'0';   
		int b=number.charAt(1)-'0';    
		int c=number.charAt(2)-'0';
		
		for (int i = 1; i <=10; i++) {
			
			for (int j =1; j <=10; j++) {
				
				for (int j2 =1; j2 <=10; j2++) {
					
					System.out.println(a+""+b+""+c);
					c++;
				}
				c=1;
				b++;
			}
			a++;
		}
		
		
		
	}
	}


