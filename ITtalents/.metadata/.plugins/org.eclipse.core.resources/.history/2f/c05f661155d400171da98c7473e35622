import java.util.Scanner;
public class Task10 {
	
public static void main(String[] args) {
     Scanner sc=new Scanner(System.in);
	int[]arr=new int[7];
	int counter=0;
	int result=0;
	for (int i = 0; i < arr.length; i++) {
		System.out.println("Enter number:");
		arr[i]=sc.nextInt();
		counter+=arr[i];
	}
	result=counter/arr.length;
	System.out.println(result);
	
}
}
