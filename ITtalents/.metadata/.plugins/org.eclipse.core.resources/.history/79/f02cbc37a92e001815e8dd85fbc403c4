package Shops;

public abstract class CommercialSite {
	private String addres;
	private String workingHours;
	private int area;
	protected int tax;
	
	public CommercialSite(String addres, String workingHours, int area) {
		this.addres = addres;
		this.workingHours = workingHours;
		this.area = area;
	}
	
	

}
