
public class task23 {
public static void main(String[] args) {
	int result=0;
	int num=1;
	int counter=1;
	while (counter!=10) {
		
	if (counter==num) {
		
		System.out.print(num+"*"+counter+"; ");
		
	}
	counter++;
		if (counter==10) {
			num++;
			counter=1;
			System.out.println();
		}
	
		if (num==10) {
			break;
		}
	
	}
}
}
